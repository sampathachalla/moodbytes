{
  "expo": {
    "name": "ModeBytes",
    "slug": "ModeBytes",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "modebytes",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splashscreen_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moodbytes.app"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.moodbytes.app",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "102ee9e7-deb8-4cc0-ac8a-791d9f6bdf30"
      }
    },
    "owner": "sampath2001"
  }
}
